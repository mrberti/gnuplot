set terminal svg dynamic
set output '/var/www/html/simon/temps/temps.tmp.svg'
set datafile separator ','
set xdata time
set timefmt "%s"
set format x "%m/%d %H:%M"
set grid
set xtics 432000
set xtics rotate
set ytics 1
set title "temps.csv"
n=3
plot for [i=2:(n+1)] "<tail -n 72000 /home/simon/temps.csv" using ($1+(9*3600)):i with lines notitle,\
#"<tail -n 4320 /home/simon/temps.csv" using ($1+(9*3600)):($3-$2) with lines notitle
